        "            for sub_item in sub_items[:3]:\n",
        "                print(f\"      - {sub_item}\")\n",
        "            if len(sub_items) > 3:\n",
        "                print(f\"      ... and {len(sub_items) - 3} more\")\n",
        "        else:\n",
        "            print(f\"   üìÑ {item}\")\n",
        "else:\n",
        "    print(f\"‚ùå {data_path} does not exist\")"
